{
  "executable": "Data2Code",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "Data2Code",
  "author": "Andrei Ignat, Alexandru Badita",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "www.data-to-code.eu",
    "productName": "Data2Code",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.1",
    "compression": "maximum",
    "directories": {
      "output": "C:\\electron-dist\\data-to-code"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}
